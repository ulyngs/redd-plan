{
    "name": "redd-map",
    "version": "1.0.0",
    "description": "ReDD Map - Danish 6-month calendar view for planning your year",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "dev": "electron . --dev",
        "build": "node build.js",
        "build:mac": "node build.js --mac",
